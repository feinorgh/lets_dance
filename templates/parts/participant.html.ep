% layout undef; # no layout, this is a fragment
<div class="row">
  <div class="col-md-6 participant-presentation">
    <h1 class="participant-name"><%= $info->{name} %></h1>
    <img src="<%= $info->{image}{url} %>" class="img-responsive img-rounded" alt="">
    <p><%= $info->{description} %></p>
  </div>
  <div class="col-md-6">
    <div class="videolist" data-person-tag="<%= $info->{person_tag} %>"></div>
  </div>
</div>
<hr>
<%#
vim: ts=2 sw=2
%>
